<!--mine_new.wxml-->
<view class="page">
    <view class="page__bd">
        
        <view class="weui-cells weui-cells_after-title" style="margin-top:20rpx;">
            <view class="weui-cell">
                <view class="weui-cell__hd" style="position: relative;margin-right: 10px;">
                    <image src="http://www.sucaijishi.com/uploadfile/2016/0203/20160203022635285.png" style="width: 50px; height: 50px; display: block"/>
                </view>
                <view class="weui-cell__bd">
                    <view>联系人名称</view>
                    <view style="font-size: 13px;color: #888888;">摘要信息</view>
                </view>
            </view>
            <view class="weui-cell weui-cell_access">
                <view class="weui-cell__bd">
                    <view style="display: inline-block; vertical-align: middle">我发表的</view>
                </view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            <view class="weui-cell weui-cell_access">
                <view class="weui-cell__bd">
                    <view style="display: inline-block; vertical-align: middle">我收到的</view>
                    <!--<view class="weui-badge" style="margin-left: 5px;">8</view>-->
                </view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
                <!--<view class="weui-cell__ft weui-cell__ft_in-access">详细信息</view>-->
            </view>
        </view>

        <view class="weui-cells weui-cells_after-title"  style="margin-top:20rpx;">
            <view bindtap="gotoHelp" class="weui-cell weui-cell_access">
                <view class="weui-cell__bd">
                    <view style="display: inline-block; vertical-align: middle">使用帮助</view>
                </view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            <view bindtap="gotoFeedback" class="weui-cell weui-cell_access">
                <view class="weui-cell__bd">
                    <view style="display: inline-block; vertical-align: middle">问题反馈</view>
                </view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
        </view>

        <view class="weui-cells weui-cells_after-title" style="margin-top:20rpx;">
            <view class="weui-cell weui-cell_access">
                <view class="weui-cell__bd">
                    <view style="display: inline-block; vertical-align: middle">退出登录</view>
                    <!--<view class="weui-badge" style="margin-left: 5px;">New</view>-->
                </view>
            </view>
        </view>
    </view>
</view>
